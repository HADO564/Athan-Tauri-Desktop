{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "notification:default",
    "http:default",
    {
      "identifier": "notification:default",
      "allow": ["**"]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://api.aladhan.com/*"
        }
      ]
    }
  ]
}